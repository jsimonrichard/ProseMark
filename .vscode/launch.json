{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/apps/vscode-extensions/core",
        "--extensionDevelopmentPath=${workspaceFolder}/apps/vscode-extensions/cspell-integration"
